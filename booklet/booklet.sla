            <StoryText>
                <DefaultStyle/>
                <ITEXT CPARENT="Titolo" CH="Francesco: "/>
                <ITEXT CPARENT="Titolo" FONT="Gargoyle Serif Bold Italic" CH="you"/>
                <para PARENT="Titolo"/>
                <ITEXT CPARENT="prompt" CH="&gt; "/>
                <ITEXT CPARENT="prompt" FEATURES="inherit allcaps" CH="examine me"/>
                <para PARENT="prompt"/>
                <ITEXT CPARENT="Default Character Style" CH="A young man, about 30 years old, with blond hair. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CPARENT="Default Character Style" CH="You work as a software engineer, enjoy photography and love hiking in the mountains. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CPARENT="Default Character Style" CH="In love with your girlfriend, very beautiful, but also shrewish when something doesn't go her way. In the end you always please her and she appreciates it. "/>
                <para PARENT="Default Paragraph Style"/>
                <ITEXT CPARENT="Default Character Style" CH="You wear a pair of brown trekking boots, a checkered flannel shirt, a white t-shirt and a pair of beige shorts. "/>
                <para PARENT="Default Paragraph Style"/>
                <para PARENT="Default Paragraph Style"/>
