        <PAGEOBJECT XPOS="462.835645669292" YPOS="422.835645669292" OwnPage="2" ItemID="232304720" PTYPE="4" WIDTH="294.803149606299" HEIGHT="294.803149606299" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L294.803 0 L294.803 294.803 L0 294.803 L0 0 Z" copath="M0 0 L294.803 0 L294.803 294.803 L0 294.803 L0 0 Z" gXpos="462.835645669292" gYpos="422.835645669292" gWidth="0" gHeight="0" PSTYLE="Default Paragraph Style" LAYER="0" NEXTITEM="231944320" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Default Paragraph Style" LINESPMode="1" LINESP="12"/>
                <ITEXT FONT="Gargoyle Serif Bold" FONTSIZE="28" CH="Breakfast in the Dolomites"/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="11.9905511811024" NACH="6.00944881889764" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Bold Italic" FONTSIZE="16" CH="a simple walkthrought"/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="10.0062992125984" NACH="6.00944881889764" BCOLOR="None"/>
                <para PARENT="Default Paragraph Style" ALIGN="1" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="The following is a short list of actions that must be taken to complete the game. "/>
                <breakline/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="You can also ask the characters questions about objects in the game, the weather, the surroundings."/>
                <breakline/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="You can go to the buffet as often as you like, or call a waiter to your table to order more hot drinks."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Bold Italic" FONTSIZE="11" CH="Leaving the car"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Remember to take the key from the car dashboard."/>
                <para PARENT="Testo4_Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Remember to take the luggage."/>
                <para PARENT="Testo4_Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Close and lock the car."/>
                <para PARENT="Testo4_Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Go to the garden."/>
                <para PARENT="Testo4_Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="In the garden"/>
                <para PARENT="Testo4_Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="A beautiful place where to kiss your girlfriend."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="The morning after"/>
                <para PARENT="Testo4_Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Greet the receptionist."/>
                <para PARENT="Testo4_Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Go east to the dining room."/>
                <para PARENT="Testo4_Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="First access to the dining room"/>
                <para PARENT="Testo4_Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Greet a waiter."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="If you are carrying the room key put it in a pocket."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Sit down on the bench."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="When a waiter arrives order the hot drink of your choice."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="While waiting for the hot drink go north to the buffet."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Food from the buffet"/>
                <para PARENT="Testo4_Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="(remember you have a carrying capacity of only three items, over a plate you can place a maximum of ten items)"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Take a dish."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Put a few slices of bread on the dish."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Put a few knobs of butter on the dish (if you like)."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Put a few portions of jam on the plate (if you like)."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Put some slices of meat on the dish (again, if you like)."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Cooked egg"/>
                <para PARENT="Testo4_Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Greet the cook."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Choose the type of preparation you prefer."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="When your egg is ready, take the plate the cook hands you."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Don’t forget to thank the cook."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Cold drinks"/>
                <para PARENT="Testo4_Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Find a glass, asking the cook for it can help."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Open the drawers."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Take a glass."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Fill the glass with the drink of your choice."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Drinking hot drink"/>
                <para PARENT="Testo4_Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT CH="When the waiter returns with the ordered hot drink, drink it."/>
                <para PARENT="Testo4_Text body" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="(If you have ordered a tea with lemon or milk pour the content of the pot in the tea before eating.)"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Spreading butter and jam"/>
                <para PARENT="Testo4_Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Spread butter on a slice of bread (if you have got the bread)."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Open the jam jar."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Spread the jam on the bread (buttered or not at you choice)."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Put the empty jar in the can."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Eat the slice of bread. "/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Stuff bread with meat or cheese"/>
                <para PARENT="Testo4_Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Put the meat or the cheese on a slice of bread."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Eat the slice of bread."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT CH="Monica ask you to bring her a vegetble juice"/>
                <para PARENT="Testo4_Heading 4" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="No choice, answer yes."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Bold Italic" FONTSIZE="11" CH="The juicer"/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="14.1448818897638" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Take a glass (if you don’t find it, ask the cook)."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Put the glass in the juicer."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Choose a vagetable and put it on the cutting board."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Cut the vegetable with the knife."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Open the juicer."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Put the chopped vegetable in the juicer."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Switch on the juicer."/>
                <para PARENT="Default Paragraph Style" ALIGN="0" LINESPMode="1" LINESP="12" INDENT="0" RMARGIN="0" FIRST="0" VOR="0" NACH="0" BCOLOR="None"/>
                <ITEXT FONT="Gargoyle Serif Regular" FONTSIZE="9" CH="Wait until no more juice goes down in the glass."/>
